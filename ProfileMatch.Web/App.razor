        <CascadingAuthenticationState>
<Router AppAssembly="@typeof(Program).Assembly" PreferExactMatches="@true" AdditionalAssemblies="new[] { typeof(ProfileMatch.Sites.AdminDashboard).Assembly, typeof(ProfileMatch.Components.User.UserAccount).Assembly }">
    <Found Context="routeData">
        <AuthorizeRouteView RouteData="@routeData" DefaultLayout="@typeof(MainLayout)">
            <NotAuthorized>
                <RedirectToLogin></RedirectToLogin>
            </NotAuthorized>
        </AuthorizeRouteView>
    </Found>
    <NotFound>
            <LayoutView Layout="@typeof(MainLayout)">
         <PageNotFound />
            </LayoutView>
    </NotFound>
</Router>
        </CascadingAuthenticationState>